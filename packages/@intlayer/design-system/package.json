{
  "name": "@intlayer/design-system",
  "version": "3.2.0",
  "private": false,
  "keywords": [
    "design-system",
    "intlayer",
    "react",
    "ui"
  ],
  "homepage": "https://intlayer.org",
  "bugs": {
    "url": "https://github.com/aymericzip/intlayer/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aymericzip/intlayer.git"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Aymeric PINEAU",
    "url": "https://github.com/aymericzip"
  },
  "contributors": [
    {
      "name": "Aymeric Pineau",
      "email": "ay.pineau@gmail.com",
      "url": "https://github.com/aymericzip"
    }
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/components/index.mjs",
      "require": "./dist/components/index.cjs",
      "types": "./dist/components/index.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks/index.mjs",
      "require": "./dist/hooks/index.cjs",
      "types": "./dist/hooks/index.d.ts"
    },
    "./libs": {
      "import": "./dist/libs/index.mjs",
      "require": "./dist/libs/index.cjs",
      "types": "./dist/libs/index.d.ts"
    },
    "./tailwind-config": {
      "import": "./tailwind.config.js",
      "require": "./tailwind.config.js",
      "types": "./tailwind.config.d.ts"
    },
    "./css": "./dist/tailwind.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm build:package && pnpm build:css && pnpm build:storybook",
    "build:css": "tailwindcss --minify -i ./src/tailwind.css -o ./dist/tailwind.css",
    "build:package": "vite build",
    "build:storybook": "storybook build",
    "dev": "vite build --watch",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@intlayer/config": "workspace:^",
    "@intlayer/core": "workspace:^",
    "@intlayer/dictionaries-entry": "workspace:^",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "deepmerge": "^4.3.1",
    "framer-motion": "^11.11.17",
    "markdown-to-jsx": "^7.6.2",
    "react-copy-to-clipboard": "^5.1.0",
    "react-hook-form": "^7.53.2",
    "react-intlayer": "workspace:^",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.6.1",
    "rollup-preserve-directives": "^1.1.2",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.2",
    "@intlayer/backend": "workspace:^",
    "@storybook/addon-essentials": "^8.4.4",
    "@storybook/addon-interactions": "^8.4.4",
    "@storybook/addon-links": "^8.4.4",
    "@storybook/addon-onboarding": "^8.4.4",
    "@storybook/blocks": "^8.4.4",
    "@storybook/react": "^8.4.4",
    "@storybook/react-vite": "^8.4.4",
    "@storybook/test": "^8.4.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/espree": "^10.1.0",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-dom": "^18.3.1",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "@utils/ts-config": "workspace:^",
    "@utils/ts-config-types": "workspace:^",
    "@utils/tsup-config": "workspace:^",
    "@vitejs/plugin-react": "^4.3.3",
    "babel-core": "^6.26.3",
    "babel-loader": "^9.2.1",
    "concurrently": "^9.1.0",
    "eslint": "^9.14.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "glob": "^11.0.0",
    "lucide-react": "^0.456.0",
    "prettier": "^3.3.3",
    "storybook": "^8.4.4",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^4.3.0"
  },
  "peerDependencies": {
    "@intlayer/backend": "workspace:^",
    "@intlayer/config": "workspace:^",
    "@intlayer/core": "workspace:^",
    "@intlayer/dictionaries-entry": "workspace:^",
    "intlayer": "workspace:^",
    "react": ">=16.0.0 <19.0.0",
    "react-dom": ">=16.0.0 <19.0.0",
    "react-intlayer": "workspace:^",
    "zustand": "^4.5.5"
  }
}
